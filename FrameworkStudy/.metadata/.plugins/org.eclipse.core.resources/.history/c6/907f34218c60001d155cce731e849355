	// 주 업무(core concern) 진행(수행)을 위한 메소드 구현
	
	@Override
	public int add(int x, int y)
	{
		int result = 0;
		
		return result;
	}

	@Override
	public int sub(int x, int y)
	{
		int result = 0;
		
		return result;
	}

	@Override
	public int mul(int x, int y)
	{
		int result = 0;
		return result;
	}

	@Override
	public int div(int x, int y)
	{
		// TODO Auto-generated method stub
		return 0;
	}
	

}
